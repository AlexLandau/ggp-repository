<?xml version="1.0" encoding="UTF-8"?>
<cronentries>
  <cron>
    <url>/cron/update_dresden_quick</url>
    <description>Fetch new games that have been observed on the Dresden server.</description>
    <schedule>every 15 minutes</schedule>
  </cron>
  <cron>
    <url>/cron/update_dresden_full</url>
    <description>Fetch all games that have been observed on the Dresden server.</description>
    <schedule>every 24 hours</schedule>
  </cron>  
  <cron>
    <url>/cron/update_stanford_quick</url>
    <description>Fetch new games from the Stanford server.</description>
    <schedule>every 15 minutes</schedule>
  </cron>
  <cron>
    <url>/cron/update_stanford_full</url>
    <description>Fetch all games from the Stanford server.</description>
    <schedule>every 24 hours</schedule>
  </cron>  
</cronentries>